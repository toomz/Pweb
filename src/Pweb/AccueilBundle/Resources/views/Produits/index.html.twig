{% extends '::base.html.twig' %}

{% block title %} Les produits {% endblock %}

{% block body %}
	
	<h3>Vous êtes dans la section des produits</h3>
  <h4>C'est ici que vos petits yeux vont briller de mille feux</h4>
  
  <table class="tab" style="width:100%">
		<tbody>
     <tr class="tab">
      {% for i in prod %}
        <td class="tab">{{ i }}</td>
      {% endfor %}
    </tr>
    
    <tr class="tab">
      {% for i in 6..10 %}
        <td class="tab">Produit n°{{ i }}</td>
      {% endfor %}
    </tr>
      
		</tbody>

	</table>
  
{% endblock %}
